
public class Calculate {
	int result = 1;
	
	int getFactorial (int num) {
		for(int i=num; i>0; i--) {
			result *= i;
		}
		System.out.println("factorial 결과값 = " + result);
		return result = 0;
	}
	
	int getPower(int num) {
		for(int i=1;i<=num;i++) {
			result += i*i;
		}
		System.out.println("power 결과값 = " + result);
		return result = 0;
	}
}
